allow-hotplug eth1
iface eth1 inet static
        address 10.223.252.1
        netmask 255.255.255.0
        network 10.223.252.0
        broadcast 10.223.252.255
        #gateway 10.223.0.1
        ##ddns-* options are implemented by the resolvconf package, if installed
        #dns-nameservers 192.168.172.129
        #dns-search cs.niisi.ras.ru
auto eth1:1
iface eth1:1 inet static
        address 10.223.2.1
        netmask 255.255.255.0
        network 10.223.2.0
        broadcast 10.223.0.255

